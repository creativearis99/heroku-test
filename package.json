{
  "name": "heroku-test",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.jsx",
  "scripts": {
    "dev": "npm run start:client & npm run start:server",
    "start": "node dist/server.js",
    "start:client": "webpack-dev-server --config webpack.config.client.js",
    "start:server": "webpack -w --config webpack.config.server.js",
    "clean": "rimraf dist/",
    "build": "npm run clean && npm run build:server & npm run build:client",
    "build:server": "webpack --env.prod --config webpack.config.server.js",
    "build:client": "webpack --env.prod --config webpack.config.client.js",
    "lint": "eslint --ext js,jsx src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy:test": "npm run lint && npm run test",
    "deploy:build": "npm run deploy:test && npm run build && docker build -t heroku-test . ",
    "deploy:run": "docker run -p 7000:7000 heroku-test",
    "deploy:compose": "npm run deploy:test && npm run build && docker-compose up",
    "deploy:up": "docker run -p 7000:7000 heroku-test",
    "debug:server": "webpack --config webpack.config.server.js -w",
    "debug:client": "webpack --config webpack.config.client.js -w",
    "debug": "npm run debug:server",
    "dist": "npm run build && cd dist && npm start"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": {
    "name": "Yuri Krupnik",
    "email": "krupnik.yuri@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "@material-ui/core": "^4.9.3",
    "axios": "^1.6.4",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "flexboxgrid": "^6.3.1",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-loadable": "^5.5.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.1.0",
    "generate-json-webpack-plugin": "^0.3.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.13.1",
    "nodemon-webpack-plugin": "^4.3.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always",
    "printWidth": 100,
    "tabWidth": 4
  },
  "husky": {
    "hooks": {
      "post-checkout": "npm run pi"
    }
  }
}
